{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny":false,
    "removeComments":true,
    "target": "es5",
    "moduleResolution": "node",
    "outDir": "./server",
    "resolveJsonModule": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules","./src/public"]
}
